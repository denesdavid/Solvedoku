                                              StandardColors="{Binding Path=PuzzleColorList}"
                                              ShowTabHeaders="False" ShowAvailableColors="False"
                                              SelectedColor="LightBlue"
                                              StandardColorsHeader="Elérhető színek:"
                                              AdvancedButtonHeader="Egyéb színek" Style="{DynamicResource PuzzleColorPickerStyle}"
                                              ></wpfx:ColorPicker>
                                <Button x:Name="BtDrawPuzzle" Content="Rajzol" Click="BtDrawPuzzle_OnClickPuzzle_OnClick"></Button>
                                <Button x:Name="BtSolvePuzzle" Content="Megoldás" Click="BtSolvePuzzle_OnClick"></Button>
                                <Button x:Name="BtSavePuzzle"  Content="Mentés..." Click="BtSavePuzzle_OnClick"></Button>
                                <Button x:Name="BtLoadPuzzle"  Content="Betöltés..." Click="BtLoadPuzzle_OnClick"></Button>
                                <Label x:Name="LbPuzzleSolvesCount" Content="Megoldások: 1/200" HorizontalAlignment="Center" FontSize="24" Foreground="AntiqueWhite" Margin="8,8,8,8" Visibility="Hidden"></Label>
                            </StackPanel>
                            <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Center">
                                <Button  x:Name="BtPuzzleLeft" Click="BtPuzzleLeft_OnClick" Width="50" IsEnabled="False">
