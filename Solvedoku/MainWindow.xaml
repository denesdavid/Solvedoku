                                </Button>
                                <Button  x:Name="BtPuzzleRight" Click="BtPuzzleRight_OnClick" Width="50" IsEnabled="False">
                                    <Image Width="45" Height="45" Source="Resources/Icons/RightArrow.png"/>
                                </Button>
