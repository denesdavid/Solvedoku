        <DockPanel x:Name="ClassicDockPanel">
            <StackPanel DockPanel.Dock="Right">
                <Label Content="Válassz méretet:" Margin="4,4,4,0"/>
                <ComboBox x:Name="CboxClassicSizes" Margin="8,8,8,8" SelectedIndex="0" DockPanel.Dock="Right">
                    <ComboBoxItem x:Name="Cbi3399" Tag="9x9x3x3" Content="3x3 (9x9)"/>
                    <ComboBoxItem x:Name="Cbi2244" Tag="4x4x2x2" Content="2x2 (4x4)"/>
                    <ComboBoxItem x:Name="Cbi2366" Tag="6x6x2x3" Content="2x3 (6x6)"/>
                </ComboBox>
                <Button x:Name="BtDrawClassic" DockPanel.Dock="Right" Content="Rajzol" Style="{DynamicResource ButtonStyle}"/>
                <Button x:Name="BtSolveClassic" Content="Megoldás" Style="{DynamicResource ButtonStyle}"/>
                <Button x:Name="BtSaveClassic" Content="Mentés..." Style="{DynamicResource ButtonStyle}"/>
                <Button x:Name="BtLoadClassic" Content="Betöltés..." Style="{DynamicResource ButtonStyle}"/>
                <Label x:Name="LbClassicSolvesCount" Content="Megoldások: 1/200" HorizontalAlignment="Center" FontSize="24" Foreground="AntiqueWhite" Margin="8,8,8,8" Visibility="Hidden"/>
            </StackPanel>

            <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Center">
                <Button  x:Name="BtClassicLeft" Style="{DynamicResource ButtonStyle}" Width="50" IsEnabled="False">
                    <Image Width="45" Height="45" Source="../../Resources/Icons/left_arrow.png"/>
                </Button>
                <Button  x:Name="BtClassicRight" Style="{DynamicResource ButtonStyle}" Width="50" IsEnabled="False">
                    <Image Width="45" Height="45" Source="../../Resources/Icons/right_arrow.png"/>
                </Button>
            </StackPanel>

